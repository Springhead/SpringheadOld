				Name="VCLinkerTool"
				AdditionalDependencies="$(RUBY_LIB)"
				OutputFile="../ptm.dll"
				LinkIncremental="2"
				AdditionalLibraryDirectories="&quot;../..\lib\win32&quot;"
				GenerateDebugInformation="TRUE"
				ProgramDatabaseFile="$(OutDir)/RBPTM.pdb"
				SubSystem="2"
				ImportLibrary="$(OutDir)/RBPTM.lib"
				TargetMachine="1"/>
			<Tool
				Name="VCMIDLTool"/>
			<Tool
				Name="VCPostBuildEventTool"/>
			<Tool
				Name="VCPreBuildEventTool"
				Description="Swigでラッパーコードを自動生成する"
				CommandLine="$(SWIG)/swig -ruby -c++ -I../../include -I../../ruby -o RBPTM.cpp RBPTM.i"/>
			<Tool
				Name="VCPreLinkEventTool"/>
			<Tool
				Name="VCResourceCompilerTool"/>
